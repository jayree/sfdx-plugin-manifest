{
  "name": "@jayree/sfdx-plugin-manifest",
  "description": "jayree sfdx manifest commands",
  "version": "2.1.7",
  "author": "jayree",
  "bugs": "https://github.com/jayree/sfdx-plugin-manifest/issues",
  "dependencies": {
    "@oclif/config": "^1.18.2",
    "@salesforce/command": "^4.2.1",
    "@salesforce/core": "^2.33.1",
    "@salesforce/kit": "^1.5.22",
    "@salesforce/source-deploy-retrieve": "^5.8.2",
    "@salesforce/ts-types": "^1.5.20",
    "cli-ux": "^6.0.7",
    "debug": "^4.3.3",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^3.21.1",
    "fs-extra": "^10.0.0",
    "isomorphic-git": "^1.10.3",
    "jsforce": "^1.11.0",
    "listr2": "^3.13.5",
    "marked": "^4.0.8",
    "marked-terminal": "^4.2.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@oclif/dev-cli": "^1.26.10",
    "@salesforce/dev-config": "^3.0.0",
    "@salesforce/dev-scripts": "^2.0.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/chai": "^4.3.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jsforce": "^1.9.37",
    "@types/marked": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.4",
    "@types/sinon": "10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "eslint-config-oclif": "^4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "^37.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sinon": "12.0.1",
    "source-map-support": "~0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14.17.1"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/bin/is-sfdx.js"
  ],
  "homepage": "https://github.com/jayree/sfdx-plugin-manifest",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/changelog"
    },
    "topics": {
      "jayree": {
        "description": "manifest, source, and org automation toolset"
      },
      "jayree:manifest": {
        "description": "generate and manipulate manifest files"
      },
      "jayree:manifest:git": {
        "description": "generate manifest files based on git data"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-plugin-manifest",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif-dev manifest && oclif-dev readme",
    "postinstall": "node ./bin/is-sfdx.js || patch-package",
    "postpack": "rimraf oclif.manifest.json",
    "postpublish": "pinst --enable",
    "prepack": "sf-prepack",
    "prepare": "is-ci || node ./bin/is-sfdx.js || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif-dev manifest && oclif-dev readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
