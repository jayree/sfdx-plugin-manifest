{
  "name": "@jayree/sfdx-plugin-manifest",
  "description": "jayree sfdx manifest commands",
  "version": "2.2.1",
  "author": "jayree",
  "bugs": "https://github.com/jayree/sfdx-plugin-manifest/issues",
  "dependencies": {
    "@oclif/config": "^1.18.3",
    "@salesforce/command": "^4.2.2",
    "@salesforce/kit": "^1.5.41",
    "@salesforce/source-deploy-retrieve": "^5.15.0",
    "@salesforce/ts-types": "^1.5.20",
    "cli-ux": "^5.6.7",
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.0.7",
    "fs-extra": "^10.1.0",
    "isomorphic-git": "^1.17.2",
    "jsforce": "^1.11.0",
    "listr2": "^4.0.5",
    "marked": "^4.0.15",
    "marked-terminal": "^4.2.0",
    "semver": "^7.3.7",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-command-snapshot": "^3",
    "@oclif/plugin-help": "^5.1.12",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^2.0.2",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jsforce": "^1.9.42",
    "@types/marked": "^4.0.3",
    "@types/marked-terminal": "^3.1.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.33",
    "@types/semver": "^7.3.9",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-oclif": "^4.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "14.0.0",
    "source-map-support": "~0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.17.1"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md"
  ],
  "homepage": "https://github.com/jayree/sfdx-plugin-manifest",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/changelog"
    },
    "topics": {
      "jayree": {
        "description": "manifest, source, and org automation toolset"
      },
      "jayree:manifest": {
        "description": "generate and manipulate manifest files"
      },
      "jayree:manifest:git": {
        "description": "generate manifest files based on git data"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ]
  },
  "repository": "jayree/sfdx-plugin-manifest",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif-dev readme",
    "postinstall": "patch-package",
    "postpack": "shx rm -f oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "yarn lint && yarn test:deprecation-policy",
    "prepack": "sf-prepack",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:deprecation-policy": "./bin/run snapshot:compare",
    "version": "oclif-dev readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
