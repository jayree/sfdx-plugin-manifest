{
  "name": "@jayree/sfdx-plugin-manifest",
  "description": "A Salesforce CLI plugin containing commands for creating manifest files from Salesforce orgs or git commits of sfdx projects.",
  "version": "2.7.3",
  "author": "jayree",
  "type": "module",
  "bugs": "https://github.com/jayree/sfdx-plugin-manifest/issues",
  "dependencies": {
    "@oclif/core": "^1.24.0",
    "@salesforce/command": "^5.2.41",
    "@salesforce/kit": "^1.8.3",
    "@salesforce/sf-plugins-core": "^1.22.2",
    "@salesforce/source-deploy-retrieve": "^7.7.5",
    "@salesforce/ts-types": "^1.7.2",
    "debug": "^4.3.4",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.0.13",
    "fs-extra": "^11.1.0",
    "graceful-fs": "^4.2.10",
    "isomorphic-git": "^1.21.0",
    "listr2": "^5.0.7",
    "marked": "^4.2.12",
    "marked-terminal": "^5.1.1",
    "semver": "^7.3.8",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@oclif/plugin-command-snapshot": "^3.3.0",
    "@oclif/plugin-help": "^5.2.0",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.4",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/marked": "^4.0.8",
    "@types/marked-terminal": "^3.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/semver": "^7.3.13",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-oclif": "^4.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "^39.6.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sf-plugin": "^1.7.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.6.0",
    "patch-package": "^6.5.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "15.0.1",
    "source-map-support": "~0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/CHANGELOG.md",
    "/bin/is-sfdx.js",
    "/schemas"
  ],
  "homepage": "https://github.com/jayree/sfdx-plugin-manifest",
  "keywords": [
    "git",
    "diff",
    "delta",
    "manifest",
    "salesforce",
    "package",
    "retrieve",
    "deploy",
    "sfdx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/changelog"
    },
    "topics": {
      "jayree": {
        "description": "manifest, source, and org automation toolset",
        "subtopics": {
          "manifest": {
            "description": "generate and manipulate manifest files",
            "subtopics": {
              "git": {
                "description": "generate manifest files based on git data"
              }
            }
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/@jayree/sfdx-plugin-manifest/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/sfdx-plugin-manifest/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/sfdx-plugin-manifest",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif readme",
    "postinstall": "node ./bin/is-sfdx.js || patch-package",
    "postpack": "shx rm -f oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema",
    "prepack": "sf-prepack",
    "prepare": "is-ci || node ./bin/is-sfdx.js || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:deprecation-policy": "./bin/dev.js snapshot:compare",
    "test:json-schema": "./bin/dev.js schema:compare",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
